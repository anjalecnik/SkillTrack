	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"migration:run": "typeorm-ts-node-commonjs -d src/libs/db/db.cli.ts migration:run",
		"migration:generate": "typeorm-ts-node-commonjs -d src/libs/db/db.cli.ts migration:generate",
		"migration:revert": "typeorm-ts-node-commonjs -d src/libs/db/db.cli.ts migration:revert",
		"migration:move": "mv -i -v 1[0-9]*[-]*.ts src/libs/db/migrations",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
