		"@nestjs/terminus": "^10.2.0",
		"@nestjs/typeorm": "10.0.2",
		"@typescript-eslint/parser": "^8.33.0",
		"cache-manager": "^5.7.6",
		"cache-manager-ioredis": "^2.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"date-holidays": "^3.23.12",
		"dayjs": "^1.11.13",
